{
  "name": "@exbrain/common-react",
  "version": "1.0.0",
  "description": "Shared React components, utilities, and styles for React projects",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./logger": {
      "import": "./dist/logger.esm.js",
      "require": "./dist/logger.cjs.js",
      "types": "./dist/logger.d.ts"
    },
    "./styles/index.css": "./dist/styles/index.css",
    "./styles/*": "./dist/styles/*",
    "./utils/auth-events": {
      "import": "./dist/utils/auth-events.esm.js",
      "require": "./dist/utils/auth-events.cjs.js",
      "types": "./dist/utils/auth-events.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist && tsc --emitDeclarationOnly --outDir dist/utils src/utils/auth-events.ts --declaration --declarationMap --esModuleInterop --skipLibCheck && cp -r src/styles dist/styles",
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^27.4.0",
    "pino-pretty": "^13.1.3",
    "typescript": "^4.9.5",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "utilities"
  ],
  "author": "Company Team",
  "license": "MIT",
  "dependencies": {
    "pino": "^10.2.0"
  }
}
